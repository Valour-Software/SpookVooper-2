@model Group
@if (Model.ImageUrl.Contains("https://")) {
    var owner = Model!.Owner!;

    @if (Model.GroupType == GroupTypes.PoliticalParty)
    {
        <img class="govpfp m-2" style="max-width:90%; max-height:90%; box-shadow: 0px 0px 0px 7px #1bd9f2;" src="@Model.ImageUrl" />
    }
    else
    {
        <img class="govpfp" style="max-width:90%; max-height:90%;" src="@Model.ImageUrl" />
    }
}